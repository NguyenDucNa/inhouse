{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "type-check": "tsc",
    "dev": "vite",
    "dev:with-local-server": "concurrently \"vite\" \"npm run generate -- --watch --config codegen.dev.ts \" ",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix-lint": "eslint --ext ts,tsx --fix . ",
    "preview": "vite preview",
    "generate": "graphql-codegen",
    "codegen": "graphql-codegen --config -v -d codegen.ts",
    "codegen:local": " && npm run codegen"
  },
  "dependencies": {
    "tsconfig":"workspace:*",
    "eslint-config-custom":"workspace:*",
    "network-graphql": "workspace:*",
    "core":"workspace:*",
    "ui":"workspace:*",
    "@apollo/client": "^3.8.8",
    "@heroicons/react": "^2.0.18",
    "@nestjs/event-emitter": "^2.0.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.91.0",
    "classnames": "^2.3.2",
    "date-fns": "^3.0.6",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-image-file-resizer": "^0.4.8",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.4.1",
    "@headlessui/react": "^1.7.16",
    "immer": "^10.0.3",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@parcel/watcher": "^2.3.0",
    "@tailwindcss/forms": "^0.5.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "@headlessui/tailwindcss": "^0.2.0"
  }
}
